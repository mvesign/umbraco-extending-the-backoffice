{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "019c1df4-aace-7b88-acdd-6d4074e22c11",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "DisableDeleteWhenReferenced": true,
        "DisableUnpublishWhenReferenced": true
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsNamespace": "UmbracoExtendingTheBackoffice.Models.Content",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "~/Models/Content/Generated",
        "IncludeVersionNumberInGeneratedModels": false
      }
    }
  },
  "uSync": {
    "Settings": {
      "AddOnPing": false,
      "DefaultSet": "Default",
      "ExportAtStartup": "None",
      "ExportOnSave": "None",
      "ImportAtStartup": "None",
      "ImportOnFirstBoot": false,
      "FailOnMissingParent": true,
      "FailOnDuplicates": true,
      "UiEnabledGroups": "Settings",
      "Folders": [ "uSync/UmbracoExtendingTheBackoffice" ]
    },
    "Sets": {
      "Default": {
        "IsSelectable": false,
        "DisabledHandlers": [
          "PublishedContentHandler",
          "MediaFileHandler",
          "MemberHandler",
          "MemberGroupHandler",
          "UserHandler",
          "UserGroupHandler",
          "PublicAccessHandler",
          "PartialFileHandler",
          "PartialMacroFileHandler",
          "ScriptFileHandler",
          "StyleSheetHandler",
          "DomainHandler",
          "MediaHandler",
          "ContentHandler",
          "ContentTemplateHandler"
        ],
        "HandlerDefaults": {
          "FailOnMissingParent": true
        },
        "Handlers": {
          "DictionaryHandler": {
            "Group": "Settings",
            "CreateOnly": true
          }
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}